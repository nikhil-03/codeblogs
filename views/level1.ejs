<div class="content-body">

    <h2 data-aos="zoom-out-down" class="input-blog-heading">BEGINEER'S-CONTENTS</h2>
<h4 data-aos="zoom-out-down" class="page-details">This level is for the one who has not coded even once in his entire life . This level covers input/output , if/else , while loop 
    , for loop and all important standard libraries.</h4>
<hr>
    <% found.forEach(function(blogs){ %> 

        <div class="content-main">
           <div>
              <h4 data-aos="fade-up" class="content-title"> <%= blogs.topic %></h4>
           </div>
           <hr>
           <div class="content-content">
               <p data-aos="fade-up"> <%= blogs.theory %>  </p>
           </div>
           <hr>
           <div class="content-link">  
               <a href="<%=blogs.link%>"><p  data-aos="fade-right" class="content-url">LINK  </p> </a>
           </div>
           <div class="content-link">  
               <a href="<%=blogs.ytlink%>"><p  data-aos="fade-right" class="content-url">YOUTUBE LINK : <%= blogs.ytlink %></p> </a>
          </div>

           <div>
               <a href="https://wa.me/917979047466?text=<%=blogs.topic%>:%20<%=blogs.theory%>%20<%=blogs._id%>:%20Need%20help->"><p data-aos="fade-right">Need help ? <i class="fa fa-whatsapp" aria-hidden="true"></i> </p></a>
               <!-- <%var str=blogs.createdAt%>  -->
               <p><%=blogs.createdAt.toDateString()%></p>
           </div>
           <hr class="login-hr">
        </div>
    
    <% }) %> 
